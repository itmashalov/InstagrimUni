<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>Servlets.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ImageOperationsServlet</servlet-name>
        <servlet-class>Servlets.ImageOperationsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadImgServlet</servlet-name>
        <servlet-class>Servlets.UploadImgServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchImagesServlet</servlet-name>
        <servlet-class>Servlets.SearchImagesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchUsersServlet</servlet-name>
        <servlet-class>Servlets.SearchUsersServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchFriendsServlet</servlet-name>
        <servlet-class>Servlets.SearchFriendsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SendFriendRequestServlet</servlet-name>
        <servlet-class>Servlets.SendFriendRequestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Authenticator</servlet-name>
        <servlet-class>Servlets.Authenticator</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProfileOperationsServlet</servlet-name>
        <servlet-class>Servlets.ProfileOperationsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/RegisterServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>Servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>Servlets.LogoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>GalleryServlet</servlet-name>
        <servlet-class>Servlets.GalleryServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GalleryServlet</servlet-name>
        <url-pattern>/GalleryServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ImageOperationsServlet</servlet-name>
        <url-pattern>/ImageOperationsServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadImgServlet</servlet-name>
        <url-pattern>/UploadImgServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchImagesServlet</servlet-name>
        <url-pattern>/SearchImagesServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchUsersServlet</servlet-name>
        <url-pattern>/SearchUsersServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchFriendsServlet</servlet-name>
        <url-pattern>/SearchFriendsServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SendFriendRequestServlet</servlet-name>
        <url-pattern>/SendFriendRequestServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Authenticator</servlet-name>
        <url-pattern>/Authenticator</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProfileOperationsServlet</servlet-name>
        <url-pattern>/ProfileOperationsServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
        <servlet>
            <servlet-name>gettime</servlet-name>
            <servlet-class>gettime</servlet-class>
        </servlet>
        <servlet-mapping>
            <servlet-name>gettime</servlet-name>
            <url-pattern>/gettime</url-pattern>
        </servlet-mapping>
    </session-config>
</web-app>
